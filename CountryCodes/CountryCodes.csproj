<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <PackageId>CountryCodesLib</PackageId>
    <Title>Country Codes Utility</Title>
    <Authors>Mohan Raj Senthilnathan</Authors>
    <Description>The Country Codes Library is a distributable library that provides a comprehensive list of two-letter and three-letter country codes in accordance with the ISO 3166-1 standard. The data in this library has been collected from reliable sources and is regularly updated to ensure accuracy.</Description>
    <RepositoryUrl>https://github.com/mohansenthil/CountryCodes</RepositoryUrl>
    <Copyright>none</Copyright>
    <PackageTags>twolettercountrycode;countrycode;threelettercountrycode</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

</Project>
